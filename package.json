{
  "name": "tawnia-healthcare-analytics",
  "version": "2.0.0",
  "description": "AI-powered healthcare insurance data analysis platform",
  "main": "main.py",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "webpack --mode production",
    "build:backend": "echo 'Backend build complete'",
    "deploy": "npm run build && npm run deploy:cloudflare",
    "deploy:cloudflare": "wrangler publish"
  },
  "dependencies": {
    "express": "^4.19.2",
