    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "winston": "^3.13.0",
    "dotenv": "^16.4.5",
    "uuid": "^10.0.0",
    "xlsx": "^0.18.5",
    "csv-parser": "^3.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "@cloudflare/workers-types": "^4.20240529.0",
    "wrangler": "^4.33.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
